<h1> "Student Form" </h1>

<%= form_tag new_student_path do %> 

  <label> First name: </label><br>
   <%= text_field_tag id="first_name" :"student['first_name']"%> <br> 
   
  <label> Last name: </label> <br> 
   <%= text_field_tag id="last_name" :"student['last_name']"%> <br> 
   
   
  <%= submit_tag "Submit Student" %>
  
  <%=params.inspect%> 
  
<%end%> 
  