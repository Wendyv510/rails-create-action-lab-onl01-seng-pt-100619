<h1> "Student Form" </h1>

<%= form_tag students_path do %> 

  <label> First name: </label><br>
   <%= text_field_tag id="first_name" :first_name%> <br> 
   
  <label> Last name: </label> <br> 
   <%= text_field_tag id="last_name" :last_name%> <br> 
   
   
  <%= submit_tag "Submit Student" %>
  
  <%=params.inspect%> 
  
<%end%> 
  